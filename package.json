{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --parallel build:*",
    "dev:server": "nodemon --watch build --exec node \"build/bundle.js\"",
    "dev:build-server": "webpack --mode=development --config webpack.server.js --watch",
    "dev:build-client": "webpack --mode=development --config webpack.client.js --watch",
    "build:server": "webpack --mode=production --config webpack.server.js",
    "build:client": "webpack --mode=production --config webpack.client.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "^1.7.4",
    "concurrently": "3.5.0",
    "express": "^4.17.1",
    "express-http-proxy": "1.0.6",
    "lodash": "^4.17.15",
    "node-localstorage": "^2.1.5",
    "nodemon": "^2.0.2",
    "npm-run-all": "4.1.1",
    "react": "^16.12.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.12.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "^2.1.2",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.10"
  }
}
